{
    "manifestVersion": 1,
    "id": "dnn-pipeline-tasks",
    "name": "DNN Pipeline Tasks",
    "version": "2.3.0",
    "publisher": "iowacomputergurus",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Azure Pipeline tools to assist with DevOps when working with DNN/Evoq based solutions.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "tags": [
        "DNN",
        "EVOQ",
        "DNN Manifest"
    ],
    "galleryFlags":["Public"],
    "content": {
        "details": {
            "path": "readme.md"
        },
        "license": {
            "path": "license-terms.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://www.iowacomputergurus.com"
        },
        "support": {
            "uri": "https://support.iowacomputergurus.com"
        },
        "repository": {
            "uri": "https://github.com/IowaComputerGurus/Azure-DevOps-Extensions"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/IowaComputerGurus/Azure-DevOps-Extensions"
    },
    "files": [
        {
            "path": "SetDnnVersionTask"
        },
        {
            "path": "ExtractExtensionInstallerArtifacts"
        }
    ],
    "contributions": [
        {
            "id": "set-dnn-manifest-version",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "SetDnnVersionTask"
            }
        },
        {
            "id": "extract-dnn-extension-install-artifects",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ExtractExtensionInstallerArtifacts"
            }
        }
    ]
}